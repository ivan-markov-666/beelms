#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../jest@29.7.0_@types+node@20.19.4_ts-node@10.9.2_@types+node@20.19.4_typescript@5.8.3_/node_modules/jest/bin/jest.js" "$@"
else
  exec node  "$basedir/../../../../../jest@29.7.0_@types+node@20.19.4_ts-node@10.9.2_@types+node@20.19.4_typescript@5.8.3_/node_modules/jest/bin/jest.js" "$@"
fi
