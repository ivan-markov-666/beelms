#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../ts-node@10.9.2_@types+node@20.19.4_typescript@5.8.3/node_modules/ts-node/dist/bin-esm.js" "$@"
else
  exec node  "$basedir/../../../../../ts-node@10.9.2_@types+node@20.19.4_typescript@5.8.3/node_modules/ts-node/dist/bin-esm.js" "$@"
fi
