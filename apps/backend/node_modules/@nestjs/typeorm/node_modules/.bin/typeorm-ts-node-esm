#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../typeorm@0.3.25_better-sqlite3@11.0.0_pg@8.16.3_reflect-metadata@0.1.14_sqlite3@5.1.7_ts-node@_6v3tvnkvn5reeq7ndk4cy7rxy4/node_modules/typeorm/cli-ts-node-esm.js" "$@"
else
  exec node  "$basedir/../../../../../../typeorm@0.3.25_better-sqlite3@11.0.0_pg@8.16.3_reflect-metadata@0.1.14_sqlite3@5.1.7_ts-node@_6v3tvnkvn5reeq7ndk4cy7rxy4/node_modules/typeorm/cli-ts-node-esm.js" "$@"
fi
