#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/ts-jest@29.4.0_@babel+core@7.28.0_@jest+transform@30.0.4_@jest+types@30.0.1_babel-jest@29.7.0_t2utndxldispbcltnwul3iuwci/node_modules/ts-jest/cli.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/ts-jest@29.4.0_@babel+core@7.28.0_@jest+transform@30.0.4_@jest+types@30.0.1_babel-jest@29.7.0_t2utndxldispbcltnwul3iuwci/node_modules/ts-jest/cli.js" "$@"
fi
